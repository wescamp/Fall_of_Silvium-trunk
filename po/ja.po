msgstr ""

#. [unit]: type=Wild Elf Thunderblade, id=Dubhain
#: Fall_of_Silvium/scenarios/6t_Mount_Stormkeep.cfg:123
msgid "Dubhain"
msgstr ""

#. [unit]: type=Wild Elf Thunderblade, id=Ceallayach
#: Fall_of_Silvium/scenarios/6t_Mount_Stormkeep.cfg:130
msgid "Ceallayach"
msgstr ""

#. [unit]: type=Wild Elf Thunderblade, id=Oisan
#: Fall_of_Silvium/scenarios/6t_Mount_Stormkeep.cfg:137
msgid "Oisan"
msgstr ""

#. [unit]: type=Wild Elf Thunderblade, id=Daithi
